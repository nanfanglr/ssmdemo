<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.syt.dao.CityDao">
    <!-- 目的：为dao接口方法提供sql语句配置 -->
    <select id="queryCity" resultType="City" >
        <!-- 具体的sql -->
        SELECT
            _id,
            _name,
            _sort_key,
            _hot
        FROM
            city
     
    </select>
</mapper>